GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Firebase" "${PODS_ROOT}/Headers/Public/TwilioSDK"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Firebase" $(inherited) "${PODS_ROOT}/TwilioSDK/Libraries" $(inherited) "${PODS_ROOT}/TwilioSDK/Libraries" $(inherited) "${PODS_ROOT}/TwilioSDK/Libraries"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Firebase" -isystem "${PODS_ROOT}/Headers/Public/TwilioSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -read_only_relocs suppress -l"Firebase" -l"FirebaseStatic" -l"TwilioClient" -l"c++" -l"crypto" -l"icucore" -l"ssl" -framework "AVFoundation" -framework "AudioToolbox" -framework "CFNetwork" -framework "Security" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods